<%= simple_nested_form_for(@fraction, @structure_fraction) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :parliament_member, label: "Nama Anggota" %>
    <%= f.input :level, label: "Jabatan Dalam  Fraksi" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
