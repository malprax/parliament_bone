<%= simple_form_for(@comission) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :code, label: "Kode" %>
    <%= f.association :parliament_member, label_method: :fraksi_gabung_nama, label: "Anggota"  %>
    <%= f.input :comment, label: "Keterangan" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
