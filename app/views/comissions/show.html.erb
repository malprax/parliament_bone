
<div class="container">
  <div class="row">
    <p>
      <strong>Nama:</strong>
      <%= @comission.name %>
    </p>

    <p>
      <strong>Kode:</strong>
      <%= @comission.code %>
    </p>

    <p>
      <strong>Keterangan:</strong>
      <%= @comission.comment %>
    </p>
    <p>
      <strong>Fraction:</strong>
      <%= @comission.fraction.name rescue nil %>
    </p>



    <%= link_to 'Edit', edit_comission_path(@comission) %> |
    <%= link_to 'Back', comissions_path %>

  </div>
</div>
