
<%= simple_form_for(@parliament) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :avatar, as: :image_preview, input_html: {preview_version: :thumb}, label: "Foto Anggota" %>
    <%= f.input :name, label: "Nama" %>
    <%= f.input :tempat_lahir, label: "Tempat Lahir" %>
    <%= f.input :tanggal_lahir, label: "Tanggal Lahir" %>
    <%= f.association :dapil, label: "Dapil" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn-success" %>
  </div>
<% end %>

<br>
