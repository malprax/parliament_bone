<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-8 col-md-8">
        <% @parliaments.each do |parliament| %>
        <h3 class="lead"><%= parliament.name %></h3>

        <% end %>
    </div>
    <div class="hidden-xs col-sm-4 col-md-4">
        <% @fractions.each do |fraction| %>
            <%= link_to fraction.name, fraction_path(fraction.id) %>
        <% end %>
    </div>
  </div>
</div>
