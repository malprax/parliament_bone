<%= simple_form_for @fraction do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :name, label: "Nama"%>
    <%= f.association :parliament_member, label: "Nama Anggota Fraksi" %>
    <%= f.association :level, as: :radio_buttons, label: "Pangkat / Jabatan" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
