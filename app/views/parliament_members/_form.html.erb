<div class="container">
  <div class="row">
    <%= simple_form_for(@parliament_member) do |f| %>
      <%= f.error_notification %>

      <div class="form-inputs">
        <%= f.input :name, label: "Nama" %>
        <%= f.input :fraction_id, label: "Fraksi" %>
        <%= f.input :level_id, label:"Jabatan Fraksi" %>
        <%= f.input :dapil, label: "Dapil" %>
      </div>

      <div class="form-actions">
        <%= f.button :submit %>
      </div>
    <% end %>
  </div>
</div>
