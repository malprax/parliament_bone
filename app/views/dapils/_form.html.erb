<%= simple_form_for(@dapil) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, label:'Nama' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class:'btn btn-success' %>&nbsp;&nbsp;  <%= link_to 'Back', dapils_path, class:'btn btn-default' %>
  </div>
<% end %>
